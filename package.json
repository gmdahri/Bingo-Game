{
  "name": "bingo-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "node src/server/websocket.js",
    "start:server": "node src/server/websocket.js",
    "expose": "ngrok http 3000 --host-header=localhost:3000",
    "expose:ws": "ngrok tcp 8080",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "tunnel": "concurrently \"npm run expose\" \"npm run expose:ws\""
  },
  "dependencies": {
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
